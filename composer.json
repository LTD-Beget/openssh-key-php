{
    "name": "dimistor/openssh-key",
    "type": "library",
    "license": "MIT",
    "description": "Parse ssh keys in OpenSSH format",
    "authors": [
        {
            "name": "Dmitry Ryabov"
        }
    ],
    "require": {
        "php": ">=7.1",
        "marc-mabe/php-enum": "^2.0 || ^3.0",
        "dimistor/stringstream": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": { "OpenSSH\\":  "src/"}
    },
    "autoload-dev": {
        "psr-4": { "OpenSSH\\tests\\":  "tests/"}
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dimistor/stringstream"
        }
    ]
}
